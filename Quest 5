Ex 14 Quest 5"
package main

import "fmt"

func ToLower(s string) string {
	result := ""
	for _, char := range s {
		if char >= 'A' && char <= 'Z' {
			char -= 'A' - 'a'
		}
		result += string(char)
	}
	return result
}
func main() {
	fmt.Println(piscine.ToLower("Hello! How are you?"))
}
